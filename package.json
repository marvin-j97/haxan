{
  "name": "haxan",
  "version": "0.7.0",
  "description": "Intuitive HTTP client for browsers and servers",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "coverage": "vitest run --coverage",
    "doc": "rm -rf docs && typedoc --out docs src && touch docs/.nojekyll",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix --ext .ts",
    "test": "vitest run",
    "test:open": "vitest --ui",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marvin-j97/haxan.git"
  },
  "keywords": [
    "http",
    "ajax",
    "node",
    "browser",
    "xhr",
    "promise",
    "fetch"
  ],
  "author": "marvin-j97",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marvin-j97/haxan/issues"
  },
  "homepage": "https://github.com/marvin-j97/haxan#readme",
  "devDependencies": {
    "@tinyhttp/app": "2.0.22",
    "@tinyhttp/logger": "1.3.0",
    "@types/node": "18.14.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "@vitest/coverage-istanbul": "0.22.1",
    "@vitest/ui": "0.22.1",
    "abort-controller": "3.0.0",
    "cross-fetch": "3.1.4",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.5.0",
    "milliparsec": "2.2.2",
    "nyc": "15.1.0",
    "ts-node": "10.9.1",
    "tsup": "6.2.2",
    "typedoc": "0.22.18",
    "typescript": "4.0.2",
    "vitest": "0.22.1"
  }
}
